<h1>Eat-Da-Burger!</h1>
{{#each burgers}}
<p>{{id}}.&nbsp{{burger_name}}</p>
{{/each}}

<h2>Add a Burger</h2>
<form class="create-form">
    <div class="form-group">
        <label for="burger">Burger Name:</label>
        <input type="text" id="burger" name="burger_name">
    </div>

<button type="submit">Add a Burger</button>    
</form>